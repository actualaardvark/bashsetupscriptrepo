mkdir /volumes/"install macos ventura"/"Additional Scripts"

cd /volumes/"install macos ventura"/"Additional Scripts"

curl "https://raw.githubusercontent.com/actualaardvark/bashsetupscriptrepo/main/Additional%20Scripts/serial" > serial

chmod +x serial

curl "https://raw.githubusercontent.com/actualaardvark/bashsetupscriptrepo/main/Additional%20Scripts/users" > users

chmod +x users
